{
    "builds": [
      {
        "src": "frontend/next.config.mjs",
        "use": "@vercel/next"
      },
      {
        "src": "backend/index.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend/index.ts"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/$1"
      }
    ]
  }